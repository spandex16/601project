   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "pokemon_df = pd.read_csv('Pokemon.csv')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "182973f9-c180-4fd9-a1eb-0f6c96337f11",
   "metadata": {},
   "outputs": [],
   "source": [
    "pokemon_df['Type Count'] = pokemon_df['Type 2'].apply(lambda x: 'Dual-Type' if pd.notnull(x) else 'Single-Type')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "04c06c42-91fc-40fc-9274-356404a0df14",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1400x800 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "\n",
    "# Classify Pok√©mon as 'Single-Type' or 'Dual-Type'\n",
    "pokemon_df['Type Count'] = pokemon_df['Type 2'].apply(lambda x: 'Dual-Type' if pd.notnull(x) else 'Single-Type')\n",
    "\n",
    "# Mean Calculation\n",
    "numeric_cols = pokemon_df.select_dtypes(include=['float64', 'int64']).columns.drop('Generation')\n",
    "\n",
    "# Calculate average stats for Single-Type and Dual-Type Pok√©mon\n",
    "avg_stats_by_type = pokemon_df.groupby('Type Count')[numeric_cols].mean()\n",
    "\n",
    "# Plot\n",
    "plt.figure(figsize=(14, 8))\n",
    "for stat in avg_stats_by_type.columns:\n",
    "    sns.barplot(x=avg_stats_by_type.index, y=avg_stats_by_type[stat], label=stat)\n",
    "\n",
    "plt.title('Average Stats for Single-Type vs Dual-Type Pok√©mon')\n",
    "plt.ylabel('Average Stat Value')\n",
    "plt.legend(bbox_to_anchor=(1.05, 1), loc='upper left')\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
